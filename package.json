    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "cordova-android": "^8.1.0",
    "cordova-plugin-usb-event": "file:plugins_src/cordova-plugin-usb-event",
    "net.kyosho.usb": "file:plugins_src/cordova-plugin-usb-event",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.12",
